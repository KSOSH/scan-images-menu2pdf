{
	"name":        "scan-images-menu2pdf",
	"version":     "2.2.0",
	"description": "",
	"main":        "index.js",
	"scripts": {
		"test":       "node main.js",
		"prod":       "node index.js",
		"build_js":   "minify main.js > index.js",
		"pycalendar": "pyinstaller --icon ./favicon.ico --add-data ./favicon.ico;. --specpath ./modules/calendar --name calendar --hidden-import babel.numbers --onefile --console --clean --distpath ./modules/calendar/dists modules/calendar/_calendar.py",
		"pymenu":     "pyinstaller --icon ./favicon.ico --add-data ./favicon.ico;. --specpath ./modules/typemenu --name typemenu --hidden-import babel.numbers --onefile --console --clean --distpath ./modules/typemenu/dists modules/typemenu/typemenu.py",
		"pydialog":   "pyinstaller --icon ./favicon.ico --add-data ./favicon.ico;. --specpath ./modules/opendialog --name opendialog --hidden-import babel.numbers --onefile --console --clean --distpath ./modules/opendialog/dists modules/opendialog/opendialog.py",
		"python":     "npm run pycalendar && npm run pymenu && npm run pydialog",
		"build":      "minify main.js > index.js && npm run pycalendar && npm run pymenu && npm run pydialog"
	},
	"files": [
		"index.js",
		"main.js",
		"menu.json",
		"modules/**/*"
	],
	"author":  "ProjectSoft <projectsoft2009@yandex.ru>",
	"license": "MIT",
	"dependencies": {
		"colors":          "^1.4.0",
		"compress-images": "^2.0.5",
		"gifsicle":        "^5.2.1",
		"pngquant-bin":    "^6.0.1"
	}
}
